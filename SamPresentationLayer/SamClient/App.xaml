<Application x:Class="SamClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SamClient"
             StartupUri="Views/Windows/Main.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Themes/Default/Colors.xaml" />
                <ResourceDictionary Source="Resources/Themes/Default/Dimens.xaml" />
                <ResourceDictionary Source="Resources/Themes/Default/Fonts.xaml" />
                <ResourceDictionary Source="Resources/Themes/Default/ControlTemplates.xaml" />
                <ResourceDictionary Source="Resources/Themes/Default/Types.xaml" />
                <ResourceDictionary Source="Resources/Themes/Default/Styles.xaml" />
                <ResourceDictionary Source="Resources/Themes/Default/Layout.xaml" />
                <ResourceDictionary Source="Resources/Themes/Default/ToolkitStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
