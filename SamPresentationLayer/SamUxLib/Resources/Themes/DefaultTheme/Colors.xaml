<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Primary-->
    <SolidColorBrush x:Key="color_primary" Color="#f2c812"/>
    <SolidColorBrush x:Key="color_primary_light" Color="#fce16a"/>
    <SolidColorBrush x:Key="color_primary_dark" Color="#db8c27"/>

    <!--Accent-->
    <SolidColorBrush x:Key="color_accent" Color="#282829"/>
    <SolidColorBrush x:Key="color_accent_light" Color="#3c3f41" />

    <!--Neutral-->
    <SolidColorBrush x:Key="color_neutral" Color="#ffffff"/>
    <SolidColorBrush x:Key="color_neutral_dark" Color="#e6e7e8"/>
    <SolidColorBrush x:Key="color_neutral_darker" Color="#cccccc"/>
    <SolidColorBrush x:Key="color_neutral_darkest" Color="#959595" />

    <!--Others-->
    <SolidColorBrush x:Key="color_disabled" Color="#e6e7e8"/>
    <SolidColorBrush x:Key="color_disabled_contrast" Color="#676767"/>
    <SolidColorBrush x:Key="color_required_field" Color="#ff0000" />
    <SolidColorBrush x:Key="color_player_bg" Color="#000" />

</ResourceDictionary>